struct Person {
    age: int,
    height: int,
}

invariant Person {
    self.age > 0
}

fn get_age(p: &mut Person) -> int {
    let x = p.age;
    get_age(p);
    p.age + p.age
}

fn init() -> Person {
    let p = Person { age: 12, height: 13 };
    assert p.age == 12;
    p.age = p.age + 1;
    p
}
